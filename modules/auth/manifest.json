{
	"slug": "",
	"apiSlug": "auth",

	"loginForm": {
		"tabs": [
			{
				"objects": [
					"email",
					"password"
				]
			}
		],

		"fields": {
			"email": {
				"type": "email",
				"label": "Email",
				"placeholder": "e.g. john.doe@example.com",
				"autofocus": true,
				"required": true
			},
			"password": {
				"type": "password",
				"label": "Password",
				"placeholder": "e.g. Cpt.Mc4wes0me!",
				"required": true
			}
		}
	}
}
